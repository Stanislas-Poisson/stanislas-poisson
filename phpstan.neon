includes:
  - ./vendor/nunomaduro/larastan/extension.neon

parameters:
  level: 8
  parallel:
    maximumNumberOfProcesses: 1
  paths:
    - app
  ignoreErrors:
  excludePaths:
    - ./.docker
    - ./.github
    - ./bootstrap/cache
    - ./node_modules
    - ./public
    - ./storage
    - ./vendor
    - ./_ide_helper.php
    - ./.phpstorm.meta.php
    - ./.php_cs
  checkMissingIterableValueType: false
